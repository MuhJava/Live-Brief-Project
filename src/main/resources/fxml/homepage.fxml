<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.sdk11element010.homepageController"
            prefHeight="400.0" prefWidth="600.0">
        <TabPane fx:id="tabpane" prefHeight="1700" prefWidth="1700" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="viewtab" text="View profile" onSelectionChanged ="#changeTab" >
                    <fx:include fx:id="clientform" source="twoclientform.fxml" />
                </Tab>
                <Tab fx:id="edittab" text="Edit profile" onSelectionChanged ="#changeTab" >
                    <fx:include fx:id="clientform1" source="twoclientform.fxml"/>
                </Tab>
                <Tab fx:id="createtab" text="Create scenario" onSelectionChanged ="#changeTab" >
                    <fx:include fx:id="scenarioform" source="scenarioform.fxml"/>
                </Tab>
                <Tab fx:id="generatetab" text="Generate report">
                </Tab>
            </tabs>
        </TabPane>
    </AnchorPane>
